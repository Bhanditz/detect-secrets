default:
  plugins:
    HexHighEntropyString: 3
    Base64HighEntropyString: 4.5
    PrivateKeyDetector: true
  baseline: .secrets.baseline
  base_tmp_dir: /tmp/detect_secrets_tracked_repos
  file_exclude_regex: ^(\.git|build|logs|node_modules|virtualenv_run)|.*tests/.*
  line_exlude_regex: # ?pragma: ?whitelist[ -]secret
  certain_extensions_line_exclude_regexes:
    ps1:
      - "checksum.*="
